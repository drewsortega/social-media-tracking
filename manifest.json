{
    "name": "Social Media Usage Tracker",
    "version": "0.1",
    "description": "CS361 Spring 2019 Project",
    "permissions": [
        "storage"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "page_action": {
        "default_popup": "html/index.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "64": "images/icon64.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "manifest_version": 2
}